<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="tictactoe.controller.ResultScreenController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/Background.jpg" />
         </image>
      </ImageView>
      <MediaView fx:id="winnerScrn" fitHeight="270.0" fitWidth="600.0" />
      <Button fx:id="homeBtn" layoutX="190.0" layoutY="300.0" mnemonicParsing="false" onAction="#homeHandler" style="-fx-background-color: #f4a460;" text="Home" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="restartBtn" layoutX="335.0" layoutY="300.0" mnemonicParsing="false" onAction="#restartHandler" style="-fx-background-color: #f4a460;" text="Restart" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
