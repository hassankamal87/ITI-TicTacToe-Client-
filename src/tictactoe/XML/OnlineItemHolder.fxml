<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="45.0" prefWidth="284.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.controller.OnlineItemHolderController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="400.0">
         <children>
            <Label fx:id="playerName" prefHeight="24.0" prefWidth="131.0" text="Player Name" textFill="#b45f06">
               <HBox.margin>
                  <Insets left="5.0" right="150.0" />
               </HBox.margin>
               <font>
                  <Font name="Cooper Black" size="20.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#inviteFriend" prefHeight="30.0" prefWidth="105.0" style="-fx-background-color: #B45F06;" text="Invite" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
</AnchorPane>
