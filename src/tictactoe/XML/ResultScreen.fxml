<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="410.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.controller.ResultScreenController">
   <children>
      <ImageView fitHeight="410.0" fitWidth="610.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/Background.jpg" />
         </image>
      </ImageView>
      <MediaView fx:id="winnerScrn" fitHeight="300.0" fitWidth="610.0" preserveRatio="false" />
      <Button fx:id="homeBtn" layoutX="197.0" layoutY="331.0" mnemonicParsing="false" onAction="#homeHandler" style="-fx-background-color: #f4a460;" text="Home" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="restartBtn" layoutX="342.0" layoutY="331.0" mnemonicParsing="false" onAction="#restartHandler" style="-fx-background-color: #f4a460;" text="Restart" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
